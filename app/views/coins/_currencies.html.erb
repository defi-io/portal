<% @currencies.each do |c| %>
	<div class='text-3xl text-slate-500 pb-5'>
		<h2><%= link_to @latest_price.other_amount(c), "/#{@latest_price.symbol.downcase}-to-#{c}", alt: "#{@latest_price.symbol} to #{c.upcase}" %>
		<span class='text-slate-400 text-xl'> <%= c.upcase %>/<%= @latest_price.symbol %></span>
		</h2>
	</div>
<% end %>